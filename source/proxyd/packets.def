//////////////////////////
// serverbound packets
//////////////////////////

PACKET_BEGIN(Handshake, 0x00)
PACKET_FIELD_VARINT(version, 32)
PACKET_FIELD_STRING(address)
PACKET_FIELD_INT(port, 16)
PACKET_FIELD_VARINT(nextState, 32)
PACKET_END()

PACKET_BEGIN(LoginStart, 0x00)
PACKET_FIELD_STRING(name)
PACKET_END()

PACKET_BEGIN(TeleportConfirm, 0x00)
PACKET_FIELD_VARINT(teleportId, 32)
PACKET_END()

PACKET_BEGIN(ChatClient, 0x03)
PACKET_FIELD_STRING(message)
PACKET_END()

PACKET_BEGIN(ClientSettings, 0x05)
PACKET_FIELD_STRING(locale)
PACKET_FIELD_INT(viewDistance, 8)
PACKET_FIELD_VARINT(chatMode, 32)
PACKET_FIELD_BOOL(chatColors)
PACKET_FIELD_UINT(displayedSkinParts, 8)
PACKET_FIELD_VARINT(mainHand, 32)
PACKET_END()

PACKET_BEGIN(CloseWindowClient, 0x0a)
PACKET_FIELD_UINT(windowId, 8)
PACKET_END()

PACKET_BEGIN(PluginMessageClient, 0x0b)
PACKET_FIELD_STRING(channel)
PACKET_FIELD_IMPLICIT_TAILBYTES(data)
PACKET_END()

PACKET_BEGIN(InteractEntity, 0x0e)
PACKET_FIELD_VARINT(entityId, 32)
PACKET_FIELD_VARINT(type, 32)
PACKET_FIELD_IMPLICIT_TAILBYTES(tail)
PACKET_END()

PACKET_BEGIN(KeepAliveClient, 0x0f)
PACKET_FIELD_INT(keepAliveId, 64)
PACKET_END()

PACKET_BEGIN(PlayerPosition, 0x11)
PACKET_FIELD_FLOAT(x, 64)
PACKET_FIELD_FLOAT(y, 64)
PACKET_FIELD_FLOAT(z, 64)
PACKET_FIELD_BOOL(onGround)
PACKET_END()

PACKET_BEGIN(PlayerPositionRotationClient, 0x12)
PACKET_FIELD_FLOAT(x, 64)
PACKET_FIELD_FLOAT(y, 64)
PACKET_FIELD_FLOAT(z, 64)
PACKET_FIELD_FLOAT(yaw, 32)
PACKET_FIELD_FLOAT(pitch, 32)
PACKET_FIELD_BOOL(onGround)
PACKET_END()

PACKET_BEGIN(PlayerRotation, 0x13)
PACKET_FIELD_FLOAT(yaw, 32)
PACKET_FIELD_FLOAT(pitch, 32)
PACKET_FIELD_BOOL(onGround)
PACKET_END()

PACKET_BEGIN(PlayerMovement, 0x14)
PACKET_FIELD_BOOL(onGround)
PACKET_END()

PACKET_BEGIN(PlayerAbilitiesClient, 0x19)
PACKET_FIELD_UINT(flags, 8)
PACKET_FIELD_FLOAT(flyingSpeed, 32)
PACKET_FIELD_FLOAT(walkingSpeed, 32)
PACKET_END()

PACKET_BEGIN(PlayerDigging, 0x1a)
PACKET_FIELD_VARINT(status, 32)
PACKET_FIELD_UINT(location, 64)
PACKET_FIELD_INT(face, 8)
PACKET_END()

PACKET_BEGIN(EntityAction, 0x1b)
PACKET_FIELD_VARINT(entityId, 32)
PACKET_FIELD_VARINT(actionId, 32)
PACKET_FIELD_VARINT(jumpBoost, 32)
PACKET_END()

PACKET_BEGIN(HeldItemChangeClient, 0x23)
PACKET_FIELD_INT(slot, 16)
PACKET_END()

PACKET_BEGIN(AnimationClient, 0x2a)
PACKET_FIELD_VARINT(hand, 32)
PACKET_END()

PACKET_BEGIN(UseItem, 0x2d)
PACKET_FIELD_VARINT(hand, 32)
PACKET_END()

//////////////////////////
// clientbound packets
//////////////////////////

PACKET_BEGIN(DisconnectLogin, 0x00)
PACKET_FIELD_STRING(reason)
PACKET_END()

PACKET_BEGIN(LoginSuccess, 0x02)
PACKET_FIELD_STRING(uuid)
PACKET_FIELD_STRING(username)
PACKET_END()

PACKET_BEGIN(SpawnPlayer, 0x05)
PACKET_FIELD_VARINT(entityId, 32)
PACKET_FIELD_UUID(uuid)
PACKET_FIELD_FLOAT(x, 64)
PACKET_FIELD_FLOAT(y, 64)
PACKET_FIELD_FLOAT(z, 64)
PACKET_FIELD_UINT(yaw, 8)
PACKET_FIELD_UINT(pitch, 8)
PACKET_FIELD_ENTITY_METADATA(metadata)
PACKET_END()

PACKET_BEGIN(EntityAnimation, 0x06)
PACKET_FIELD_VARINT(entityId, 32)
PACKET_FIELD_UINT(animationId, 8)
PACKET_END()

PACKET_BEGIN(BlockChange, 0x0b)
PACKET_FIELD_UINT(location, 64)
PACKET_FIELD_VARINT(blockId, 32)
PACKET_END()

PACKET_BEGIN(ChatServer, 0x0e)
PACKET_FIELD_STRING(chat)
PACKET_FIELD_INT(position, 8)
PACKET_END()

PACKET_BEGIN(PluginMessageServer, 0x18)
PACKET_FIELD_STRING(channel)
PACKET_FIELD_IMPLICIT_TAILBYTES(data)
PACKET_END()

PACKET_BEGIN(Disconnect, 0x1a)
PACKET_FIELD_STRING(reason)
PACKET_END()

PACKET_BEGIN(UnloadChunk, 0x1d)
PACKET_FIELD_INT(chunkX, 32)
PACKET_FIELD_INT(chunkZ, 32)
PACKET_END()

PACKET_BEGIN(KeepAliveServer, 0x20)
PACKET_FIELD_INT(keepAliveId, 64)
PACKET_END()

PACKET_BEGIN(ChunkData, 0x21)
PACKET_FIELD_INT(x, 32)
PACKET_FIELD_INT(z, 32)
PACKET_FIELD_BOOL(fullChunk)
PACKET_FIELD_VARINT(primaryBitmask, 32)
PACKET_FIELD_NBT(heightmaps)
PACKET_FIELD_VARBYTES(data)
PACKET_FIELD_ARRAY(blockEntities,
	PACKET_FIELD_NBT(entity)
)
PACKET_END()

PACKET_BEGIN(JoinGame, 0x25)
PACKET_FIELD_INT(entityId, 32)
PACKET_FIELD_UINT(gameMode, 8)
PACKET_FIELD_INT(dimension, 32)
PACKET_FIELD_UINT(maxPlayers, 8)
PACKET_FIELD_STRING(levelType)
PACKET_FIELD_VARINT(viewDistance, 32)
PACKET_FIELD_BOOL(reducedDebugInfo)
PACKET_END()

PACKET_BEGIN(EntityMove, 0x28)
PACKET_FIELD_VARINT(entityId, 32)
PACKET_FIELD_INT(dx, 16)
PACKET_FIELD_INT(dy, 16)
PACKET_FIELD_INT(dz, 16)
PACKET_FIELD_BOOL(onGround)
PACKET_END()

PACKET_BEGIN(EntityMoveRotation, 0x29)
PACKET_FIELD_VARINT(entityId, 32)
PACKET_FIELD_INT(dx, 16)
PACKET_FIELD_INT(dy, 16)
PACKET_FIELD_INT(dz, 16)
PACKET_FIELD_UINT(yaw, 8)
PACKET_FIELD_UINT(pitch, 8)
PACKET_FIELD_BOOL(onGround)
PACKET_END()

PACKET_BEGIN(EntityRotation, 0x2a)
PACKET_FIELD_VARINT(entityId, 32)
PACKET_FIELD_UINT(yaw, 8)
PACKET_FIELD_UINT(pitch, 8)
PACKET_FIELD_BOOL(onGround)
PACKET_END()

PACKET_BEGIN(PlayerAbilitiesServer, 0x31)
PACKET_FIELD_UINT(flags, 8)
PACKET_FIELD_FLOAT(flyingSpeed, 32)
PACKET_FIELD_FLOAT(walkingSpeed, 32)
PACKET_END()

PACKET_BEGIN(PlayerPositionLookServer, 0x35)
PACKET_FIELD_FLOAT(x, 64)
PACKET_FIELD_FLOAT(y, 64)
PACKET_FIELD_FLOAT(z, 64)
PACKET_FIELD_FLOAT(yaw, 32)
PACKET_FIELD_FLOAT(pitch, 32)
PACKET_FIELD_INT(flags, 8)
PACKET_FIELD_VARINT(teleportId, 32)
PACKET_END()

PACKET_BEGIN(DestroyEntities, 0x37)
PACKET_FIELD_ARRAY(entityIds,
	PACKET_FIELD_VARINT(entityId, 32)
)
PACKET_END()

PACKET_BEGIN(EntityHeadLook, 0x3b)
PACKET_FIELD_VARINT(entityId, 32)
PACKET_FIELD_UINT(headYaw, 8)
PACKET_END()

PACKET_BEGIN(HeldItemChangeServer, 0x3f)
PACKET_FIELD_INT(slot, 8)
PACKET_END()

PACKET_BEGIN(UpdateViewPosition, 0x40)
PACKET_FIELD_VARINT(chunkX, 32)
PACKET_FIELD_VARINT(chunkZ, 32)
PACKET_END()

PACKET_BEGIN(EntityMetadata, 0x43)
PACKET_FIELD_VARINT(entityId, 32)
PACKET_FIELD_ENTITY_METADATA(metadata)
PACKET_END()

PACKET_BEGIN(SpawnPosition, 0x4d)
PACKET_FIELD_UINT(location, 64)
PACKET_END()

PACKET_BEGIN(EntityTeleport, 0x56)
PACKET_FIELD_VARINT(entityId, 32)
PACKET_FIELD_FLOAT(x, 64)
PACKET_FIELD_FLOAT(y, 64)
PACKET_FIELD_FLOAT(z, 64)
PACKET_FIELD_UINT(yaw, 8)
PACKET_FIELD_UINT(pitch, 8)
PACKET_FIELD_BOOL(onGround)
PACKET_END()

#undef PACKET_BEGIN
#undef PACKET_FIELD_BOOL
#undef PACKET_FIELD_INT
#undef PACKET_FIELD_UINT
#undef PACKET_FIELD_VARINT
#undef PACKET_FIELD_FLOAT
#undef PACKET_FIELD_STRING
#undef PACKET_FIELD_IMPLICIT_TAILBYTES
#undef PACKET_FIELD_NBT
#undef PACKET_FIELD_UUID
#undef PACKET_FIELD_VARBYTES
#undef PACKET_FIELD_ARRAY
#undef PACKET_FIELD_ENTITY_METADATA
#undef PACKET_END
